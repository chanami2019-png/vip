				while _G.AutoTanJo do
					task.wait()
					pcall(function()
						local Boss = workspace.Main.Characters["Forge Isle"].Boss:FindFirstChild("Tanjiro")
						if not (Boss and Boss:FindFirstChild("Humanoid") and Boss.Humanoid.Health > 0) then
							task.wait()
							if
								game:GetService("Players").LocalPlayer.PlayerGui.HUD.Bar.List:FindFirstChild("Quest")
							then
							else
								task.wait()
								LocalPlayer.Character.HumanoidRootPart.CFrame =
									workspace.Main.NPCs.Quests:FindFirstChild("8").HumanoidRootPart.CFrame
								VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.E, false, LocalPlayer)
								VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.E, false, LocalPlayer)
							end
							for _, Cat in
								pairs(
									workspace.Main.Characters[MonFarms.SlayersTrainee.Island][MonFarms.SlayersTrainee.MobFolder]:GetChildren()
								)
							do
								if
									Cat:IsA("Model")
									and Cat.Name == MonFarms.SlayersTrainee.Bname
									and Cat:FindFirstChild("HumanoidRootPart")
								then
									LocalPlayer.Character.HumanoidRootPart.CFrame = Cat.HumanoidRootPart.CFrame
										* CFrame.new(0, 11, 0)
										* CFrame.Angles(math.rad(-90), 0, 0)
									HitMon()
									task.wait()
								end
							end

							local text = workspace.Main.NPCs["Boss Spawn2"]["{}"].ActionText
							local current = tonumber(string.match(text, "(%d+)%s*/%s*%d+"))

							if current and current >= 50 then
								task.wait()
								LocalPlayer.Character.HumanoidRootPart.CFrame =
									workspace.Main.NPCs:FindFirstChild("Boss Spawn2").HumanoidRootPart.CFrame
								VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.E, false, LocalPlayer)
								VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.E, false, LocalPlayer)
							end
						else
							while
								Boss
								and Boss:FindFirstChild("Humanoid")
								and Boss.Humanoid.Health > 0
								and _G.AutoTanJo
							do
								task.wait()
							end
						end
					end)
				end
			end)
		end,
	})

	local HERTA_ISLAND_CFRAME = CFrame.new(
	-1030.01355, 2.30584311, -529.695312,
	0.0680051818, 2.98202796e-10, 0.997684956,
	4.95029795e-09, 1, -6.36321829e-10,
	-0.997684956, 4.98211072e-09, 0.0680051818
)

	
	local function IsOnHertaIsland()
	local hrp = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
	if not hrp then return false end

	return (hrp.Position - HERTA_ISLAND_CFRAME.Position).Magnitude < 270
end
	
local Toggle = Tanjio:AddToggle("MyToggle", {
	Title = "AutoSummonherta",
	Description = "100/100 stand on Island",
	Default = false,
	Callback = function(herta)
		_G.Autoherta = herta

		task.spawn(function()
			while _G.Autoherta do
				task.wait()

				pcall(function()
					local char = LocalPlayer.Character
					local hrp = char and char:FindFirstChild("HumanoidRootPart")
					if not hrp then return end

					-- üü¢ STEP 1 : ‡∏ñ‡πâ‡∏≤‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡∏≠‡∏¢‡∏π‡πà‡πÄ‡∏Å‡∏≤‡∏∞ ‚Üí ‡∏ß‡∏≤‡∏õ‡∏Å‡πà‡∏≠‡∏ô
					if not IsOnHertaIsland() then
						hrp.CFrame = HERTA_ISLAND_CFRAME
						task.wait(1)
						return
					end

					-- üü° STEP 2 : ‡πÄ‡∏ä‡πá‡∏Ñ‡∏ö‡∏≠‡∏™ Herta
					local Boss =
						workspace.Main.Characters["Xmas Island"]
						.Boss:FindFirstChild("Herta")

					if not (Boss and Boss:FindFirstChild("Humanoid") and Boss.Humanoid.Health > 0) then
						-- ‚ùå ‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏ö‡∏≠‡∏™ ‚Üí ‡∏ü‡∏≤‡∏£‡πå‡∏° Cat
						for _, Cat in pairs(
							workspace.Main.Characters
								[MonFarms.SnowBrawler.Island]
								[MonFarms.SnowBrawler.MobFolder]:GetChildren()
						) do
							if Cat:IsA("Model")
							and Cat.Name == MonFarms.SnowBrawler.Bname
							and Cat:FindFirstChild("Humanoid")
							and Cat.Humanoid.Health > 0
							and Cat:FindFirstChild("HumanoidRootPart") then

								hrp.CFrame =
									Cat.HumanoidRootPart.CFrame
									* CFrame.new(0, 11, 0)
									* CFrame.Angles(math.rad(-90), 0, 0)

								HitMon()
							end
						end

						-- üîµ ‡πÄ‡∏ä‡πá‡∏Ñ 100/100
						local npc = workspace.Main.NPCs:FindFirstChild("Boss Spawn4")
						if not npc then return end

						local text = npc["{}"].ActionText
						local current = tonumber(string.match(text, "(%d+)%s*/%s*%d+"))

						if current and current >= 100 then
							task.wait(0.2)
							hrp.CFrame = npc.HumanoidRootPart.CFrame

							VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.E, false, LocalPlayer)
							task.wait(0.2)
							VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.E, false, LocalPlayer)
						end
					else
						-- üõë ‡∏ö‡∏≠‡∏™‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡∏ï‡∏≤‡∏¢ ‚Üí ‡∏£‡∏≠
						while Boss 
						and Boss:FindFirstChild("Humanoid")
						and Boss.Humanoid.Health > 0
						and _G.Autoherta do
							task.wait(0.3)
						end
					end
				end)
			end
		end)
	end,
})
	
	
