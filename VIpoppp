	local Window = Fluent:CreateWindow({
		Title = "Finzer VIP",
		SubTitle = "by saintswiz_",
		TabWidth = 160,
		Size = UDim2.fromOffset(580, 460),
		Acrylic = true,
		Theme = "Amethyst",
		MinimizeKey = Enum.KeyCode.LeftControl,
	})

	local Players = game:GetService("Players")
	local LocalPlayer = Players.LocalPlayer

	local function getNumberValue(path)
		if path and path.Value ~= nil then
			return tostring(path.Value)
		end
		return "N/A"
	end

	local function getStringValue(path)
		if path and path.Value ~= nil then
			return tostring(path.Value)
		end
		return "N/A"
	end

	local function getObjectName(path)
		if path then
			return path.Name
		end
		return "None"
	end

	local Info = Window:AddTab({ Title = "Info", Icon = "info" })
	local InfoSection = Info:AddSection("Information")

	local function buildPlayerInfoText()
		local playerCurrencies = LocalPlayer:FindFirstChild("Currencys")
		local playerInfo = LocalPlayer:FindFirstChild("Info")
		local playerPity = LocalPlayer:FindFirstChild("Pity")

		local gemsValue = "N/A"
		local levelValue = "N/A"
		local raceValue = "N/A"
		local titleValue = "N/A"
		local traitValue = "N/A"
		local accessoryValue = "None"
		local pityBossValue = "N/A"
		local pityFishingValue = "N/A"

		if playerCurrencies and playerCurrencies:FindFirstChild("Gems") then
			gemsValue = getNumberValue(playerCurrencies.Gems)
		end

		if playerInfo then
			if playerInfo:FindFirstChild("Level") then
				levelValue = getNumberValue(playerInfo.Level)
			end
			if playerInfo:FindFirstChild("Race") then
				raceValue = getStringValue(playerInfo.Race)
			end
			if playerInfo:FindFirstChild("Title") then
				titleValue = getStringValue(playerInfo.Title)
			end
			if playerInfo:FindFirstChild("Trait") then
				traitValue = getStringValue(playerInfo.Trait)
			end
			if playerInfo:FindFirstChild("Accessory") then
				accessoryValue = getObjectName(playerInfo.Accessory)
			end
		end

		if playerPity then
			if playerPity:FindFirstChild("Boss") then
				pityBossValue = getNumberValue(playerPity.Boss)
			end
			if playerPity:FindFirstChild("Fishing") then
				pityFishingValue = getNumberValue(playerPity.Fishing)
			end
		end

		local textLines = {}
		table.insert(textLines, "Gem : " .. gemsValue)
		table.insert(textLines, "Level : " .. levelValue)
		table.insert(textLines, "Race : " .. raceValue)
		table.insert(textLines, "Title : " .. titleValue)
		table.insert(textLines, "Trait : " .. traitValue)
		table.insert(textLines, "Accessory : " .. accessoryValue)
		table.insert(textLines, "Pity Boss : " .. pityBossValue)
		table.insert(textLines, "Pity Fishing : " .. pityFishingValue)

		return table.concat(textLines, "\n")
	end

	local function buildKillInfoText()
		local killFolder = LocalPlayer:FindFirstChild("Kill")
		if not killFolder then
			return "Kill Data : None"
		end

		local textLines = {}
		table.insert(textLines, "Kill Data :")

		for _, killStat in pairs(killFolder:GetChildren()) do
			if killStat.Value ~= nil then
				local lineText = tostring(killStat.Name) .. " : " .. tostring(killStat.Value)
				table.insert(textLines, lineText)
			end
		end

		return table.concat(textLines, "\n")
	end

	local function buildInventoryText()
		local inventoryFolder = LocalPlayer:FindFirstChild("Inventory")
		if not inventoryFolder then
			return "Inventory : None"
		end

		local items = {}
		for _, item in pairs(inventoryFolder:GetChildren()) do
			table.insert(items, tostring(item.Name))
		end

		if #items == 0 then
			return "Inventory : None"
		end

		local textLines = {}
		table.insert(textLines, "Inventory :")
		for _, itemName in pairs(items) do
			table.insert(textLines, "- " .. itemName)
		end

		return table.concat(textLines, "\n")
	end

	local playerInfoParagraph = Info:AddParagraph({
		Title = "Player Info",
		Content = buildPlayerInfoText(),
	})

	local killInfoParagraph = Info:AddParagraph({
		Title = "Kill Stats",
		Content = buildKillInfoText(),
	})

	local inventoryParagraph = Info:AddParagraph({
		Title = "Inventory",
		Content = buildInventoryText(),
	})

	task.spawn(function()
		while true do
			task.wait(60)
			pcall(function()
				playerInfoParagraph:SetDesc(buildPlayerInfoText())
				killInfoParagraph:SetDesc(buildKillInfoText())
				inventoryParagraph:SetDesc(buildInventoryText())
			end)
		end
	end)

