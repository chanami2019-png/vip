local Toggle = Sum:AddToggle("AutoCraftAbyssalCore", {
    Title = "Auto Spawn Boss",
    Description = "Use Selected Boss From Dropdown",
    Default = false,
    Callback = function(Value)
        _G.AutoCraftAbyssalCoresss = Value

        local Players = game:GetService("Players")
        local LocalPlayer = Players.LocalPlayer

        task.spawn(function()
            while _G.AutoCraftAbyssalCoresss do
                task.wait()

                pcall(function()
                    if not selectedBossInfoss or not selectedBossInfoss.Bname then
                        return
                    end
                    local gui = LocalPlayer.PlayerGui
                    local bossUI = gui.Button and gui.Button["Boss Spawn"]
                    if not bossUI then return end
					if selectedBossInfoss.Bname == "Aizen" then
                        LocalPlayer.Character.HumanoidRootPart.CFrame =
                            CFrame.new(-466.677582, 4.84019566, -876.891479, 0.811972499, -3.93652977e-09, 0.58369565, 3.95955553e-08, 1, -4.8336787e-08, -0.58369565, 6.2359895e-08, 0.811972499)
                    VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.E, false, LocalPlayer)
                    VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.E, false, LocalPlayer)
                    end
					if selectedBossInfoss.Bname == "Kaneki" then
                        LocalPlayer.Character.HumanoidRootPart.CFrame =
                            CFrame.new(139.939407, 352.767853, -1348.19128, -0.139632791, -1.1213821e-07, -0.990203381, -2.57177035e-08, 1, -1.09621098e-07, 0.990203381, 1.01590567e-08, -0.139632791)
                    VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.E, false, LocalPlayer)
                    VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.E, false, LocalPlayer)
                    end
				    local bossButton =
                        bossUI.Frame
                        and bossUI.Frame[selectedBossInfoss.Bname]
                        and bossUI.Frame[selectedBossInfoss.Bname].Button

                    local spawnButton =
                        bossUI.Spawn
                        and bossUI.Spawn.Button

                    if bossButton then
                        interact(bossButton)
                    end

                    if spawnButton then
                        interact(spawnButton)
                    end
                end)
            end
        end)
    end,
})

