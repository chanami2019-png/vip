-- Services
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local PlayerGui = LocalPlayer:WaitForChild("PlayerGui")
local gui = PlayerGui

-- =========================
-- Set Chest Amount
-- =========================
local function setChestAmount(amount)
	local textBox = PlayerGui.Button.Confirm.Search.BG.TextBox
	if not textBox then return end

	textBox:CaptureFocus()
	task.wait(0.1)
	textBox.Text = tostring(amount)
	task.wait(0.1)
	textBox:ReleaseFocus()
end

-- =========================
-- Auto Open Chest Function
-- =========================
local function AutoOpenChest(chestName, needAmount, openAmount, toggleName)
	task.spawn(function()
		while _G[toggleName] do
			task.wait(0.5) -- ‡∏Å‡∏±‡∏ô spam

			pcall(function()
				local materialFrame =
					gui.Button.Storage_Frame.Material_Frame:FindFirstChild(chestName)
				if not materialFrame then return end

				local amountText = materialFrame.Amout.Text
				local amount = tonumber(amountText:match("%d+")) or 0
				if amount < needAmount then return end

				local buttonUI = PlayerGui:FindFirstChild("Button")
				if not buttonUI then return end

				-- ‡∏Å‡∏î‡πÄ‡∏•‡∏∑‡∏≠‡∏Å‡∏Å‡∏•‡πà‡∏≠‡∏á
				interact(materialFrame.Button)

				-- ‡∏£‡∏≠ Confirm ‡πÄ‡∏õ‡∏¥‡∏î‡∏à‡∏£‡∏¥‡∏á
				local confirm = buttonUI:WaitForChild("Confirm", 2)
				if not confirm or not confirm.Visible then return end

				task.wait(0.25)

				-- ‡πÉ‡∏™‡πà‡∏à‡∏≥‡∏ô‡∏ß‡∏ô
				setChestAmount(openAmount)
				task.wait(0.3)

				-- ‡∏Å‡∏î Accept
				if confirm.Accept and confirm.Accept:FindFirstChild("Button") then
					interact(confirm.Accept.Button)
					interact(confirm.Accept.Button)
				end

				-- ‡∏£‡∏≠‡πÉ‡∏´‡πâ UI ‡∏õ‡∏¥‡∏î
				task.wait(1)
			end)
		end
	end)
end

-- =========================
-- TOGGLES
-- =========================

-- Common Chest
Csh:AddToggle("ToggleCommonChest", {
	Title = "Common Chest",
	Default = false,
	Callback = function(v)
		_G.CommonChest = v
		if v then
			AutoOpenChest("Common Chest", 210, 200, "CommonChest")
		end
	end
})

-- Rare Chest
Csh:AddToggle("ToggleRareChest", {
	Title = "Rare Chest",
	Default = false,
	Callback = function(v)
		_G.RareChest = v
		if v then
			AutoOpenChest("Rare Chest", 210, 200, "RareChest")
		end
	end
})

-- Legendary Chest
Csh:AddToggle("ToggleLegendaryChest", {
	Title = "Legendary Chest",
	Default = false,
	Callback = function(v)
		_G.LegendaryChest = v
		if v then
			AutoOpenChest("Legendary Chest", 210, 200, "LegendaryChest")
		end
	end
})

-- Mythical Chest
Csh:AddToggle("ToggleMythicalChest", {
	Title = "Mythical Chest",
	Default = false,
	Callback = function(v)
		_G.MythicalChest = v
		if v then
			AutoOpenChest("Mythical Chest", 105, 100, "MythicalChest")
		end
	end
})

		local Dropdown = Cshh:AddDropdown("Dropdown", {
		Title = "SelectedItem",
		Description = "",
		Values = {
			"Common Chest",
			"Legendary Chest",
			"Mythical Chest",
			"Rare Chest",
			"Summon Orb"
			},
		Multi = false,
		Default = "-",
		Callback = function(value)
			selectedBuy = value
		end,
	})
local Input = Cshh:AddInput("Input", {
    Title = "Set Amout",
    Description = "number",
    Default = "20",
    Placeholder = "Put number here",
    Numeric = true,      -- ‡∏ñ‡πâ‡∏≤‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡πÉ‡∏´‡πâ‡πÄ‡∏õ‡πá‡∏ô‡∏ï‡∏±‡∏ß‡πÄ‡∏•‡∏Ç‡πÄ‡∏ó‡πà‡∏≤‡∏ô‡∏±‡πâ‡∏ô
    Finished = false,    -- ‡∏û‡∏¥‡∏°‡∏û‡πå‡πÅ‡∏•‡πâ‡∏ß‡∏™‡πà‡∏á‡∏ó‡∏±‡∏ô‡∏ó‡∏µ
