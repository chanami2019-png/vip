	local Toggle = Sword:AddToggle("MyToggle", {
		Title = "Take Damage",
		Description = "Use weapons that deal low damage.",
		Default = false,
		Callback = function(jio)
			_G.TakeDamage = jio

			if jio then
				task.spawn(function()
					while _G.TakeDamage do
						pcall(function()
							task.wait()

							LocalPlayer.Character.HumanoidRootPart.CFrame =
								workspace.Main.NPCs:FindFirstChild("Boss Spawn1").HumanoidRootPart.CFrame
							VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.E, false, LocalPlayer)
							task.wait()
							VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.E, false, LocalPlayer)

							local gui = LocalPlayer.PlayerGui.Button["Boss Spawn"]
							local summonBtn = gui.Frame:FindFirstChild("Gojo").Button
							local spawnBtn = gui.Spawn.Button
							local closeBtn = gui.Close.Button

							interact(summonBtn)
							task.wait(1)
							interact(spawnBtn)
							task.wait(1)
							interact(closeBtn)
							task.wait(1)

							local boss
							repeat
								task.wait(0.5)
								if not _G.TakeDamage then
									return
								end -- ‡∏≠‡∏≠‡∏Å‡∏ñ‡πâ‡∏≤ toggle ‡∏õ‡∏¥‡∏î
								boss = workspace.Main.Characters["Throne Isle"]["Boss"]:FindFirstChild("Gojo")
							until boss and boss:FindFirstChild("Humanoid")

							while boss and boss:FindFirstChild("Humanoid") and boss.Humanoid.Health > 0 do
								if not _G.TakeDamage then
									break
								end -- ‡∏≠‡∏≠‡∏Å‡∏ñ‡πâ‡∏≤ toggle ‡∏õ‡∏¥‡∏î
								LocalPlayer.Character.HumanoidRootPart.CFrame = boss.HumanoidRootPart.CFrame
									* CFrame.new(0, 0, -3)
									* CFrame.Angles(0, math.rad(180), 0)
								HitMon()
								task.wait(0.5)
							end

							task.wait(1)
						end)
					end
				end)
			end
		end,
	})
