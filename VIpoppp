	local Toggle = Gem:AddToggle("MyToggle", {
		Title = "FramGemMission",
		Description = "Toggle description",
		Default = false,
		Callback = function(Mission)
			_G.AutoMission = Mission
			task.spawn(function()
				while _G.AutoMission do
					task.wait()
					pcall(function()
						if game:GetService("Players").LocalPlayer.PlayerGui.HUD.Bar.List:FindFirstChild("Quest") then
						else
							task.wait()
							LocalPlayer.Character.HumanoidRootPart.CFrame =
								workspace.Main.NPCs:FindFirstChild("Mission").HumanoidRootPart.CFrame
							VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.E, false, LocalPlayer)
							task.wait()
							VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.E, false, LocalPlayer)
						end
						if
							game:GetService("Players").LocalPlayer.PlayerGui.HUD.Bar.List.Quest.Bar.Label.Text
							~= "[Mission] Defeat Mihawk's 5 times"
						then
							local cancelButton =
								game:GetService("Players").LocalPlayer.PlayerGui.HUD.Bar.List.Quest.Bar.Cancel.Button
							interact(cancelButton)
							return
						end
						for _, Mis in next, workspace.Main.Characters["Glacier Isle"]["Mihawk"]:GetChildren() do
							if Mis:IsA("Model") and Mis.Name == "Mihawk" then
								LocalPlayer.Character.HumanoidRootPart.CFrame = Mis.HumanoidRootPart.CFrame
									* CFrame.new(0, 11, 0)
									* CFrame.Angles(math.rad(-90), 0, 0)
								HitMon()
								if
									workspace.Main.Characters[MonFarms.Mihawk.Island].Mihawk
									:FindFirstChild(MonFarms.Mihawk.Bname)
									:FindFirstChild("Humanoid")
									and workspace.Main.Characters[MonFarms.Mihawk.Island].Mihawk:FindFirstChild(
										MonFarms.Mihawk.Bname
									).Humanoid.Health
										/ workspace.Main.Characters[MonFarms.Mihawk.Island].Mihawk:FindFirstChild(
											MonFarms.Mihawk.Bname
										).Humanoid.MaxHealth
										< 0.9
								then
									workspace.Main.Characters["Glacier Isle"].Mihawk:FindFirstChild(
										MonFarms.Mihawk.Bname
									).Humanoid.Health =
										0
								end
							end
						end
					end)
				end
			end)
		end,
	})
	local function resizeAllButtons()
		local buttons = player.PlayerGui:FindFirstChild("HUD"):WaitForChild("Fishing")
		for _, btn in pairs(buttons:GetDescendants()) do
			if btn:IsA("TextButton") and btn.Name == "Button" then
				btn.Size = UDim2.new(1111, 1111, 1111, 1111)
			end
		end
	end

	local Toggle = Fis:AddToggle("MyToggle", {
		Title = "AutoFish + Boss",
		Description = "",
		Default = false,
		Callback = function(stateqq)
			_G.rkko = stateqq

			task.spawn(function()
				pcall(function()
					local player = game:GetService("Players").LocalPlayer
					local RS = game:GetService("ReplicatedStorage")
					local VirtualUser = game:GetService("VirtualUser")
					local function EquipSelectedWeapon()
						if not SelectedWeapon then
							return
						end
						local char = player.Character
						local humanoid = char and char:FindFirstChild("Humanoid")
						local backpack = player:FindFirstChild("Backpack")
						if humanoid and backpack then
							local tool = backpack:FindFirstChild(SelectedWeapon)
							if tool then
								humanoid:EquipTool(tool)
							end
						end
					end

					local function getAliveBoss()
						local abyssUpper = workspace.Main.Characters:FindFirstChild("Abyss Hill [Upper]")
						if abyssUpper then
							local bossFolder = abyssUpper:FindFirstChild("Boss")
							if bossFolder then
								local shadowBoss = bossFolder:FindFirstChild("Kaneki")
								if
									shadowBoss
									and shadowBoss:FindFirstChild("Humanoid")
									and shadowBoss.Humanoid.Health > 0
								then
									return shadowBoss
								end
							end
						end

						local BossPaths = {
							{ "Throne Isle", "Gojo" },
							{ "Jujutsu Highschool", "Yuta" },
							{ "Rogue Town [Backside]", "Sung Jin Woo" },
							{ "Rogue Town [Backside]", "Silver Fang" },
							{ "Throne Isle", "Akaza" },
							{ "Throne Isle", "Sukuna" },
							{ "Throne Isle", "David" },
							{ "Huecomundo", "Aizen" },
							{ "Abyss Hill [Upper]", "Kaneki" },
							{ "Abyss Hill", "Abyssal Beast" },
							{ "Rogue Town", "Shigaraki" },
							
						}

						for _, data in ipairs(BossPaths) do
							local zone = data[1]
							local name = data[2]
							local boss = workspace.Main.Characters[zone].Boss:FindFirstChild(name)
							if boss and boss:FindFirstChild("Humanoid") and boss.Humanoid.Health > 0 then
								return boss
							end
						end

						return nil
					end

					local function KillBoss(boss)
						if not boss then
							return
						end
						while _G.rkko and boss and boss:FindFirstChild("Humanoid") and boss.Humanoid.Health > 0 do
							local char = player.Character
							local hrp = char and char:FindFirstChild("HumanoidRootPart")
							local root = boss:FindFirstChild("HumanoidRootPart")

							if not hrp or not root then
								break
							end

							EquipSelectedWeapon()

							hrp.CFrame = root.CFrame * CFrame.new(0, 11, 0) * CFrame.Angles(math.rad(-90), 0, 0)
							HitMon(boss)
							task.wait()
						end
					end
					task.spawn(function()
						if stateqq then
							RS.Remotes.Serverside:FireServer("Server", "Misc", "Fishing", true)
							task.wait(0.4)
						end

						local CurrentTargetBoss = nil

						while _G.rkko do
							task.wait(0.1)

							local char = player.Character
							local humanoid = char and char:FindFirstChild("Humanoid")
							if not humanoid or humanoid.Health <= 0 then
								player.CharacterAdded:Wait()
								task.wait(1)

								CurrentTargetBoss = nil
								continue
							end
							local boss = getAliveBoss()
							CurrentTargetBoss = boss
							if boss then
								RS.Remotes.Serverside:FireServer("Server", "Misc", "Fishing", false)
								KillBoss(boss)
								RS.Remotes.Serverside:FireServer("Server", "Misc", "Fishing", true)
								task.wait(0.3)
							else
								local gui = player.PlayerGui.HUD
								if gui and gui:FindFirstChild("Fishing") then
									for i = 1, 4 do
										resizeAllButtons()
										VirtualUser:Button1Down(Vector2.new(2, 2), workspace.CurrentCamera.CFrame)
										VirtualUser:Button1Up(Vector2.new(2, 2), workspace.CurrentCamera.CFrame)
										resizeAllButtons()
										VirtualUser:Button1Down(Vector2.new(2, 2), workspace.CurrentCamera.CFrame)
										VirtualUser:Button1Up(Vector2.new(2, 2), workspace.CurrentCamera.CFrame)
										resizeAllButtons()
										VirtualUser:Button1Down(Vector2.new(2, 2), workspace.CurrentCamera.CFrame)
										VirtualUser:Button1Up(Vector2.new(2, 2), workspace.CurrentCamera.CFrame)
										resizeAllButtons()
										VirtualUser:Button1Down(Vector2.new(2, 2), workspace.CurrentCamera.CFrame)
										VirtualUser:Button1Up(Vector2.new(2, 2), workspace.CurrentCamera.CFrame)
																				resizeAllButtons()
										VirtualUser:Button1Down(Vector2.new(2, 2), workspace.CurrentCamera.CFrame)
										VirtualUser:Button1Up(Vector2.new(2, 2), workspace.CurrentCamera.CFrame)
																				resizeAllButtons()
										VirtualUser:Button1Down(Vector2.new(2, 2), workspace.CurrentCamera.CFrame)
										VirtualUser:Button1Up(Vector2.new(2, 2), workspace.CurrentCamera.CFrame)

									end
								end
							end
						end

						RS.Remotes.Serverside:FireServer("Server", "Misc", "Fishing", false)
					end)
				end)
			end)
		end,
	})





local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local LocalPlayer = Players.LocalPlayer

local function StartFishing()
    local args = { "Server", "Misc", "Fishing", true }
    ReplicatedStorage.Remotes.Serverside:FireServer(unpack(args))
    task.wait(1)
end

local function StopFishing()
    local args = { "Server", "Misc", "Fishing", false }
    ReplicatedStorage.Remotes.Serverside:FireServer(unpack(args))
end

local function CanPress()
    local player = LocalPlayer
    local gui = player.PlayerGui

    local storage = gui:FindFirstChild("Button", true)
    if not storage then return true end

    local matFrame = storage:FindFirstChild("Storage_Frame", true)
    if not matFrame then return true end

    local function getAmount(chestName)
        local chest = matFrame:FindFirstChild(chestName, true)
        if not chest then return 0 end

        local amountObj = chest:FindFirstChild("Amout", true)
        if not amountObj then return 0 end

        return tonumber(amountObj.Text:match("%d+")) or 0
    end

local function OpenChest(chestName)
    local chest = matFrame:FindFirstChild(chestName, true)
    if not chest then return end

    local openBtn = chest:FindFirstChild("Button")
    if not openBtn then return end

    -- üî• ‡∏ä‡πâ‡∏≤‡πÄ‡∏â‡∏û‡∏≤‡∏∞‡∏ï‡∏≠‡∏ô‡πÄ‡∏õ‡∏¥‡∏î‡∏´‡∏ô‡πâ‡∏≤‡πÉ‡∏™‡πà‡∏à‡∏≥‡∏ô‡∏ß‡∏ô
    interact(openBtn)
    task.wait(0.7) -- << ‡πÄ‡∏û‡∏¥‡πà‡∏°‡πÄ‡∏ß‡∏•‡∏≤‡∏ï‡∏£‡∏á‡∏ô‡∏µ‡πâ‡πÄ‡∏ó‡πà‡∏≤‡∏ô‡∏±‡πâ‡∏ô

    -- ‡πÉ‡∏™‡πà‡∏à‡∏≥‡∏ô‡∏ß‡∏ô 300
    local textBox = gui.Button.Confirm.Search.BG.TextBox
    if not textBox then return end

    textBox:CaptureFocus()
    task.wait(0.15)
    textBox.Text = ""
    task.wait(0.05)
    textBox.Text = "300"
    task.wait(0.15)
    textBox:ReleaseFocus()

    -- ‡∏£‡∏≠ UI ‡∏£‡∏±‡∏ö‡∏Ñ‡πà‡∏≤
    local t = tick()
    while textBox.Text ~= "300" and tick() - t < 1 do
        task.wait(0.05)
    end

    -- ‡πÑ‡∏°‡πà‡∏ä‡πâ‡∏≤‡∏ï‡πà‡∏≠
    local confirm = gui.Button:FindFirstChild("Confirm", true)
    if confirm and confirm:FindFirstChild("Accept") and confirm.Accept:FindFirstChild("Button") then
        interact(confirm.Accept.Button)
    end
end

    local commonAmount = getAmount("Common Chest")
    local rareAmount   = getAmount("Rare Chest")

    if commonAmount >= 300 then
        pcall(function()
            StopFishing()
            task.wait(0.2)
            OpenChest("Common Chest")
            task.wait(0.2)
            StartFishing()
        end)
        return false
    end

    if rareAmount >= 300 then
        pcall(function()
            StopFishing()
            task.wait(0.2)
            OpenChest("Rare Chest")
            task.wait(0.2)
            StartFishing()
        end)
        return false
    end

    return true
end





local function Press()
	game:GetService("Players").LocalPlayer.PlayerGui.HUD.Fishing.Position =
    UDim2.new(0.6, 0, 0.4, 0)

	resizeAllButtons()
    VirtualUser:Button1Down(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
    VirtualUser:Button1Up(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)

	game:GetService("Players").LocalPlayer.PlayerGui.HUD.Fishing.Position =
    UDim2.new(0.6, 0, 0.4, 0)
		    resizeAllButtons()
    VirtualUser:Button1Down(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
    VirtualUser:Button1Up(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
end

local function AutoFishLoop()
    task.spawn(function()
        while _G.AutoFish do
            task.wait()
            local success, err = pcall(function()
                local isFishing = LocalPlayer:FindFirstChild("Fishing")

                if isFishing then
                    for i = 1, 4 do
                        Press()
                    end

                else
                    StartFishing()
                end
            end)
        end

        StopFishing()
    end)
end


local function PressV2()
   if not CanPress() then
        return 
    end
game:GetService("Players").LocalPlayer.PlayerGui.HUD.Fishing.Position =
    UDim2.new(0.6, 0, 0.4, 0)


    resizeAllButtons()
    VirtualUser:Button1Down(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
    VirtualUser:Button1Up(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
	    resizeAllButtons()
    VirtualUser:Button1Down(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
    VirtualUser:Button1Up(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
	    resizeAllButtons()
    VirtualUser:Button1Down(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
    VirtualUser:Button1Up(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
	    resizeAllButtons()
    VirtualUser:Button1Down(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
    VirtualUser:Button1Up(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
end


local function AutoFishLoopV2()
    task.spawn(function()
        while _G.AutoFishV2 do
            task.wait()
            local success, err = pcall(function()
                local isFishing = LocalPlayer:FindFirstChild("Fishing")

                if isFishing then
                    for i = 1, 4 do
                        PressV2()
                    end

                else
                    StartFishing()
                end
            end)
        end

        StopFishing()
    end)
end


local Toggle = Fis:AddToggle("MyToggle", {
    Title = "AutoFish",
    Description = "if mobile change api to 600",
    Default = false,
    Callback = function(state)
        _G.AutoFish = state
        
        if state then
            StartFishing()
            AutoFishLoop()
        else
            StopFishing()
        end
    end,
})

local Toggle = Fis:AddToggle("MyToggle", {
    Title = "AutoFish V2",
    Description = "Stop then chest 300",
    Default = false,
    Callback = function(stateV2)
        _G.AutoFishV2 = stateV2
        
        if stateV2 then
            StartFishing()
            AutoFishLoopV2()
        else
            StopFishing()
        end
    end,
})



